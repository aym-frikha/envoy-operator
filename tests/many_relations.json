{
  "admin": {
    "accessLogPath": "/tmp/admin_access.log",
    "address": {
      "address": "0.0.0.0",
      "portValue": "9901"
    }
  },
  "static_resources": {
    "listeners": [
      {
        "name": "listener-0",
        "address": {
          "address": "0.0.0.0",
          "portValue": "9090"
        },
        "filterChains": [
          {
            "filters": [
              {
                "name": "envoy.http_connection_manager",
                "config": {
                  "routeConfig": {
                    "name": "local_route",
                    "virtualHosts": [
                      {
                        "domains": [
                          "*"
                        ],
                        "name": "local_service",
                        "routes": [
                          {
                            "match": {
                              "prefix": "/"
                            },
                            "route": {
                              "cluster": "grpc-one",
                              "maxGrpcTimeout": "60.000s"
                            }
                          }
                        ],
                        "cors": {
                          "allowHeaders": "keep-alive",
                          "allowMethods": [
                            "GET",
                            "PUT",
                            "DELETE",
                            "POST",
                            "OPTIONS"
                          ],
                          "allowOrigin": [
                            "*"
                          ],
                          "exposeHeaders": "grpc-status,grpc-message",
                          "maxAge": "1728000"
                        }
                      }
                    ]
                  },
                  "statPrefix": "ingress_http",
                  "httpFilters": [
                    {
                      "name": "envoy.grpc_web"
                    },
                    {
                      "name": "envoy.cors"
                    },
                    {
                      "name": "envoy.router"
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      {
        "name": "listener-0",
        "address": {
          "address": "0.0.0.0",
          "portValue": "9090"
        },
        "filterChains": [
          {
            "filters": [
              {
                "name": "envoy.http_connection_manager",
                "config": {
                  "routeConfig": {
                    "name": "local_route",
                    "virtualHosts": [
                      {
                        "domains": [
                          "*"
                        ],
                        "name": "local_service",
                        "routes": [
                          {
                            "match": {
                              "prefix": "/"
                            },
                            "route": {
                              "cluster": "grpc-two",
                              "maxGrpcTimeout": "60.000s"
                            }
                          }
                        ],
                        "cors": {
                          "allowHeaders": "keep-alive",
                          "allowMethods": [
                            "GET",
                            "PUT",
                            "DELETE",
                            "POST",
                            "OPTIONS"
                          ],
                          "allowOrigin": [
                            "*"
                          ],
                          "exposeHeaders": "grpc-status,grpc-message",
                          "maxAge": "1728000"
                        }
                      }
                    ]
                  },
                  "statPrefix": "ingress_http",
                  "httpFilters": [
                    {
                      "name": "envoy.grpc_web"
                    },
